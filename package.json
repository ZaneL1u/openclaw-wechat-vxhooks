{
  "name": "openclaw-wechatify",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@10.30.0",
  "description": "OpenClaw WeChat channel plugin via Proxy API",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZaneL1u/wechatify.git"
  },
  "keywords": [
    "openclaw",
    "wechat",
    "微信",
    "chatbot",
    "ai",
    "claude"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist",
    "openclaw.plugin.json"
  ],
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "wechat",
      "label": "WeChat",
      "selectionLabel": "WeChat (微信)",
      "docsPath": "/channels/wechat",
      "docsLabel": "wechat",
      "blurb": "WeChat channel via Proxy API.",
      "order": 80
    },
    "install": {
      "npmSpec": "openclaw-wechatify",
      "localPath": ".",
      "defaultChoice": "npm"
    }
  },
  "scripts": {
    "build": "tsdown",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:unit": "vitest run test/unit",
    "test:integration": "vitest run test/integration",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "openclaw": ">=2026.2.9"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.4.3",
    "@types/node": "^22.19.11",
    "eslint": "^10.0.0",
    "openclaw": ">=2026.2.17",
    "tsdown": "^0.20.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "zod": "^4.3.6"
  }
}
